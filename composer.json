{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-curl": "^8.1",
        "ext-iconv": "*",
        "ext-json": "^8.1",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.17",
        "nelmio/api-doc-bundle": "^4.16",
        "nelmio/cors-bundle": "^2.4",
        "symfony/asset": "6.4.*",
        "symfony/console": "^6.4",
        "symfony/dotenv": "*",
        "symfony/flex": "^2.4",
        "symfony/framework-bundle": "*",
        "symfony/proxy-manager-bridge": "6.4.*",
        "symfony/routing": "6.4.*",
        "symfony/security-bundle": "*",
        "symfony/serializer": "*",
        "symfony/twig-bundle": "*",
        "symfony/yaml": "*",
        "zircote/swagger-php": "^4.8"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.52",
        "symfony/stopwatch": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
